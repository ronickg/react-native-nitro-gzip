    "nitro-codegen": "*",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-native": "^0.76.6",
    "react-native-nitro-modules": "*",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-nitro-modules": "*"
  },
